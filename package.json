{
  "name": "vlp",
  "displayName": "Valdese Lakeside Park",
  "version": "0.9.4",
  "description": "Navigating the new park and showing park plan overlays",
  "scripts": {
    "build": "webpack --mode production",
    "build-cordova": "webpack --config webpack.cordova.js --mode production",
    "make-images": "cordova-image-maker --config ./config.xml --icon ./res/icon.png --splash ./res/splash.png",
    "make-screenshot": "adb shell screencap -p | sed 's/\r$//' > screen.png",
    "cordova-browser": "cordova run browser",
    "cordova-android": "cordova run android --release",
    "debug-android": "adb logcat",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Noteworthy Software, Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/nwsw/vlp.git"
  },
  "dependencies": {
    "cordova-android": "^8.0.0",
    "cordova-browser": "^6.0.0",
    "cordova-plugin-geolocation": "^4.0.2",
    "cordova-ios": "^5.0.1",
    "cordova-plugin-statusbar": "^2.4.2",
    "leaflet": "^1.5.1"
  },
  "devDependencies": {
    "cordova-plugin-whitelist": "^1.3.4",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-geolocation": {},
      "cordova-plugin-statusbar": {}
    },
    "platforms": [
      "android",
      "browser",
      "ios"
    ]
  }
}